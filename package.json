  "scripts": {
    "start": "ice start --speedup",
    "build": "ice build",
    "surge": "yarn build && cd build && surge",
    "eslint": "eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\" --cache",
    "stylelint:fix": "npm run stylelint -- --fix",
    "codemod": "jscodeshift --extensions=tsx --parser=tsx -t scripts/jscodeshift.js src && jscodeshift --extensions=ts --parser=ts -t scripts/jscodeshift.js src",
    "tsc": "tsc -p tsconfig-pack.json",
    "bundle": "rollup -c rollup.config.mjs",
    "pack": "rm -rf build-out && npm run tsc && cp src/index.css build-out/ && cp -r public build-out/ && cp -r src/assets build-out/ && cp -r src/assets build-out/src && cp .yalcignore build-out/ && node ./scripts/update-pack.js && npm run bundle",
    "pack:watch": "tsc-watch -p tsconfig-pack.json --onSuccess \"rollup -c rollup.config.mjs\" --onFailure \"echo Beep! Compilation Failed\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "https://github.com/sleepy-zone/fabritor-web"
}
